{
  "overrides": [
    {
      "files": ["*.tsx", ".prettierrc"],
      "options": {
        "singleQuote": false,
        "printWidth": 80,
        "editor.formatOnSave": true,
        "proseWrap": "always",
        "tabWidth": 3,
        "requireConfig": false,
        "useTabs": false,
        "trailingComma": "none",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "semi": true
      }
    }
  ]
}
