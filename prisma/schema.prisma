generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Artist {
  id                        Int     @id @default(autoincrement())
  name                      String  @unique
  aliases                   Json?
  ignoreChineseCanonization Boolean @default(false) @map("ignoreChineseCanonization")
}
